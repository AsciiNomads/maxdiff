   <string notr="true">/* Modern Light Theme */

/* Style for QCheckBox */
QCheckBox {
    font-size: 14px;
    color: #2C3E50; /* Slightly darker text for better readability */
    padding: 6px;
}

QCheckBox::indicator {
    width: 20px;
    height: 20px;
    border-radius: 4px;
    border: 1px solid #BDC3C7; /* Light gray border */
    background-color: #ECF0F1; /* Light background color */
}

QCheckBox::indicator:unchecked:hover {
    background-color: #D5DBDB; /* Slightly darker on hover */
}

QCheckBox::indicator:checked {
    background-color: #3498DB; /* Blue color when checked */
    border: none;
}

/* Style for QTabWidget */
QTabWidget::pane {
    border: 1px solid #D6DBDF; /* Light border around the tab content */
    background-color: #FAFAFA; /* Light background */
}

QTabBar::tab {
    background: #F2F3F4; /* Light gray background for tabs */
    border: 1px solid #D6DBDF;
    padding: 8px 20px;
    margin-right: 3px;
    font-size: 14px;
    color: #2C3E50; /* Darker text color for better contrast */
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

QTabBar::tab:selected {
    background: #FFFFFF; /* White background for the selected tab */
    color: #1A5276; /* Darker blue for selected tab text */
    border-bottom-color: transparent; /* Removes the bottom border of the selected tab */
}

QTabBar::tab:hover {
    background: #E5E8E8; /* Light gray on hover */
}

/* Style for QLabel */
QLabel {
    font-size: 14px;

    color: #34495E; /* Darker text color */
    padding: 6px;
    /* background-color: #FFFFFF; /* White background for a clean look */
	background-color: None; 
}

QLabel#title {
    font-size: 18px;
    font-weight: bold;
    color: #1C2833; /* Darker, bold title */
    padding: 12px 0;
    background-color: #F7F9F9; /* Slightly off-white background for the title */
}
/* Modern Green Confirm Button */

QPushButton#confirm_btn {
    background-color: #28A745; /* Modern green color */
    color: #FFFFFF; /* White text */
    font-size: 16px;
    padding: 8px 16px;
    border: none;
    border-radius: 6px; /* Rounded corners for a modern look */
    font-weight: bold;
}

QPushButton#confirm_btn:hover {
    background-color: #218838; /* Darker green on hover */
}

QPushButton#confirm_btn:pressed {
    background-color: #1E7E34; /* Even darker green when pressed */
    padding-top: 10px; /* Slightly shifts down when pressed for a tactile effect */
    padding-bottom: 6px; /* Compensates for the shift */
}

QPushButton#confirm_btn:disabled {
    background-color: #A9DFBF; /* Light green when disabled */
    color: #E9ECEF; /* Light gray text */
    border: 1px solid #C3E6CB; /* Light green border when disabled */
}

/* -------------------------------- */
/* Add Button */
QPushButton#add_btn {
    background-color: #28A745; /* Modern green color for add */
    color: #FFFFFF; /* White text */
    font-size: 16px;
    padding: 8px 16px;
    border: none;
    border-radius: 6px; /* Rounded corners */
    font-weight: bold;
}

QPushButton#add_btn:hover {
    background-color: #218838; /* Darker green on hover */
}

QPushButton#add_btn:pressed {
    background-color: #1E7E34; /* Even darker green when pressed */
    padding-top: 10px; /* Simulates button press effect */
    padding-bottom: 6px;
}

QPushButton#add_btn:disabled {
    background-color: #A9DFBF; /* Light green when disabled */
    color: #E9ECEF; /* Light gray text */
    border: 1px solid #C3E6CB; /* Light green border when disabled */
}

/* Remove Button */
QPushButton#remove_btn {
    background-color: #DC3545; /* Modern red color for remove */
    color: #FFFFFF; /* White text */
    font-size: 16px;
    padding: 8px 16px;
    border: none;
    border-radius: 6px; /* Rounded corners */
    font-weight: bold;
}

QPushButton#remove_btn:hover {
    background-color: #C82333; /* Darker red on hover */
}

QPushButton#remove_btn:pressed {
    background-color: #A71D2A; /* Even darker red when pressed */
    padding-top: 10px; /* Simulates button press effect */
    padding-bottom: 6px;
}

QPushButton#remove_btn:disabled {
    background-color: #F5B7B1; /* Light red when disabled */
    color: #F8F9FA; /* Light gray text */
    border: 1px solid #E6B0AA; /* Light red border when disabled */
}
</string>
